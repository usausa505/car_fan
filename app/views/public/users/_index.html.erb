<table class='table'>
  <thead>
    <tr>
      <th>プロフィール画像</th>
      <th>ネーム</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image, size: '50x50' %></td>
        <td><%= user.name %></td>
        <td><a href=<%= "users/#{user.id}/my_page" %>>show</a></td>

      </tr>
    <% end %>
  </tbody>
</table>
